    "def selection_sort(arr):\n",
    "    n = len(arr)\n",
    "    for i in range(n):\n",
    "        min_idx = i\n",
    "        for j in range(i+1, n):\n",
    "            if arr[j] < arr[min_idx]:\n",
    "                min_idx = j\n",
    "        arr[i], arr[min_idx] = arr[min_idx], arr[i]\n",
    "IDs = [80, 1259, 1267, 2327, 2328, 3317, 4224, 4225, 4226, 4227, 4228, 4229, 4240, 4241, 4466, 4470, 4471, 4472, 4473, 4474, 4476, 4477, 4572, 4573, 4574, 4575]\n",
    "\n",
    "selection_sort(IDs)\n",
    "\n",
    "print(\"Sorted IDs:\", IDs)\n"
   ]
